<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <solid android:color="#ffffff" />

    <stroke
        android:width="10dp"
        android:color="#000000"
        android:dashWidth="2dp"
        android:dashGap="2dp" />

    <corners android:radius="100dp" />

    <padding
        android:top="8dp"
        android:bottom="8dp"
        android:left="10dp"
        android:right="10dp" />

</shape>
